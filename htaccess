RewriteEngine on


RewriteCond %{REQUEST_URI} ^.*$

RewriteRule ^([A-Za-z0-9]+)*$ "/index.php?url=$1" [L]



RewriteCond %{REQUEST_URI} ^.*$

RewriteRule ^([A-Za-z0-9]+\+)*$ "/index.php?info=1&url=$1" [L]
 


RewriteCond %{REQUEST_URI} ^.*$
RewriteRule ^([A-Za-z0-9]+\*)*$ "/index.php?qr=1&url=$1" [L]
 
